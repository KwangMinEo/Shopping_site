<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Join">	
	
	<insert id="join_add">
	  insert into users
	   values( #{user_id},#{user_name},#{user_pwd},#{user_phone},#{user_address1},#{user_address2},#{user_email},sysDate,1})
	</insert>
</mapper>
	

















